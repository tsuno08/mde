{
  "expo": {
    "name": "MDE",
    "slug": "MDE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "mimeType": "text/*"
            },
            {
              "mimeType": "text/plain+md"
            },
            {
              "mimeType": "text/plain"
            },
            {
              "mimeType": "text/markdown"
            },
            {
              "mimeType": "text/x-markdown"
            },
            {
              "scheme": "content",
              "pathPattern": ".*\\.md"
            },
            {
              "scheme": "file",
              "pathPattern": ".*\\.md"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        },
        {
          "action": "EDIT",
          "data": [
            {
              "mimeType": "text/*"
            },
            {
              "mimeType": "text/plain+md"
            },
            {
              "mimeType": "text/plain"
            },
            {
              "mimeType": "text/markdown"
            },
            {
              "mimeType": "text/x-markdown"
            },
            {
              "scheme": "content",
              "pathPattern": ".*\\.md"
            },
            {
              "scheme": "file",
              "pathPattern": ".*\\.md"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        }
      ],
      "package": "com.tuno.mde"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9cf4738b-5186-4afb-8676-581cf28ce362"
      }
    }
  }
}
